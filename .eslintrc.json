{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": [
      2,
      {
        "code": 120,
        "tabWidth": 4,
        "ignoreStrings": true
      }
    ],
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
